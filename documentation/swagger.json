  "components": {
    "schemas": {
      "securitySchemes": {
        "cookieAuth": {
          "type": "apiKey",
          "in": "cookie",
          "name": "token"
        }
      },
      "Player": {
        "type": "object",
        "properties": {
          "id": {
            "type": "string",
            "description": "Unique identifier for the player"
          },
          "name": {
            "type": "string",
            "description": "Name of the player"
          },
          "jerseyNumber": {
            "type": "integer",
            "description": "Jersey number of the player"
          }
        }
      },
      "Official": {
        "type": "object",
        "properties": {
          "fullName": {
            "type": "string",
            "example": "John Doe"
          },
          "email": {
            "type": "string",
            "format": "email",
            "example": "john.doe@example.com"
          },
          "phone": {
            "type": "string",
            "example": "+1234567890"
          },
          "description": {
            "type": "string",
            "example": "Team official description"
          },
          
          "position": {
            "type": "string",
            "enum": [
              "Owners",
              "Board of Director",
              "President/Chairman",
              "Chief Executive Officer",
              "Sporting Director",
              "Head Coach/Manager",
              "Assistant Coach",
              "Fitness Coach",
              "Condition Coach",
              "Team Doctor",
              "Equipment Manager",
              "Chief Financial Officer",
              "Event Coordinator",
              "Scouts",
              "Analyst",
              "Equipment Manager",
              "Nutritionist",
              "Goalkeeper",
              "Right Back",
              "Left Back",
              "Center Back",
              "Defensive Midfielder",
              "Central Midfielder",
              "Right Midfielder",
              "Left Midfielder",
              "Attacking Midfielder",
              "Forward/Striker"
            ],
            "example": "Head Coach/Manager"
          },
          "preferredFoot": {
            "type": "string",
            "example": "Right"
          },
          "dateOfBirth": {
            "type": "string",
            "format": "date",
            "example": "1980-01-01"
          },
          "nationality": {
            "type": "string",
            "example": "American"
          }
        },
        "required": ["fullName", "email", "phone", "position"],
        "additionalProperties": false
      },
      "Tournament": {
        "type": "object",
        "properties": {
          "id": {
            "type": "string",
            "description": "Unique identifier for the tournament"
          },
          "name": {
            "type": "string",
            "description": "Name of the tournament"
          },
          "sponsors": {
            "type": "string",
            "description": "Title sponsors of the tournament"
          },
          "startDate": {
            "type": "string",
            "format": "date",
            "description": "Start date of the tournament"
          },
          "endDate": {
            "type": "string",
            "format": "date",
            "description": "End date of the tournament"
          },
          "teams": {
            "type": "array",
            "items": {
              "$ref": "#/components/schemas/Team"
            }
          },
          "groups": {
            "type": "array",
            "items": {
              "type": "object",
              "properties": {
                "groupName": {
                  "type": "string",
                  "description": "Name of the group"
                },
                "teamIds": {
                  "type": "array",
                  "items": {
                    "type": "string",
                    "description": "ID of the teams in the group"
                  }
                }
              }
            }
          },
          "knockoutStages": {
            "type": "array",
            "items": {
              "type": "object",
              "properties": {
                "stageName": {
                  "type": "string",
                  "description": "Name of the knockout stage"
                },
                "matches": {
                  "type": "array",
                  "items": {
                    "$ref": "#/components/schemas/Match"
                  }
                }
              }
            }
          }
        }
      },
      "CreateTournament": {
        "type": "object",
        "properties": {
          "name": {
            "type": "string",
            "description": "Name of the tournament"
          },
          "sponsors": {
            "type": "string",
            "description": "Title sponsors of the tournament"
          },
          "startDate": {
            "type": "string",
            "format": "date",
            "description": "Start date of the tournament"
          },
          "endDate": {
            "type": "string",
            "format": "date",
            "description": "End date of the tournament"
          },
          "type": {
            "type": "string",
            "enum": ["Knockout", "League", "Group and Knockout"],
            "description": "Tournament type"
          }
        }
      },
      "Match": {
        "type": "object",
        "properties": {
          "id": {
            "type": "string",
            "description": "Unique identifier for the match"
          },
          "homeTeam": {
            "type": "string",
            "description": "ID of the home team"
          },
          "awayTeam": {
            "type": "string",
            "description": "ID of the away team"
          },
          "homeTeamScore": {
            "type": "integer",
            "description": "Score of the home team"
          },
          "awayTeamScore": {
            "type": "integer",
            "description": "Score of the away team"
          }
        }
      },
      "AddPlayerRequest": {
        "type": "object",
        "properties": {
          "teamId": {
            "type": "string",
            "description": "ID of the team"
          },
          "playerData": {
            "$ref": "#/components/schemas/Player"
          }
        }
      },
      "SelectJerseyNumberRequest": {
        "type": "object",
        "properties": {
          "playerId": {
            "type": "string",
            "description": "ID of the player"
          },
          "jerseyNumber": {
            "type": "integer",
            "description": "Jersey number to assign"
          }
        }
      },
      "PlayerTransferRequest": {
        "type": "object",
        "properties": {
          "playerId": {
            "type": "string",
            "description": "ID of the player requesting transfer"
          },
          "transferFee": {
            "type": "number",
            "description": "Transfer Fee"
          },
          "toTeamId": {
            "type": "string",
            "description": "ID of the team requesting the transfer"
          }
        }
      },
      "AddOfficialRequest": {
        "type": "object",
        "properties": {
          "teamId": {
            "type": "string",
            "description": "ID of the team"
          },
          "officialData": {
            "type":"array",
            "items":{
              "$ref": "#/components/schemas/Official"

            }
          }
        }
      },
     "SelectSquadRequest": {
        "type": "object",
        "properties": {
          "teamId": {
            "type": "string",
            "description": "ID of the team"
          },
          "squad": {
            "type": "array",
            "items": {
              "type": "object",
              "properties": {
                "tournamentId": {
                  "type": "string",
                  "description": "ID of the tournament"
                },
                "member": {
                  "type": "array",
                  "items": {
                   "teamId": {
            "type": "string",
            "description": "Members Id"
          }
                  }
                }
              },
              "required": ["tournamentId", "member"]
            }
          }
        }
      },
      "Team": {
        "type": "object",
        "properties": {
          "id": {
            "type": "string",
            "description": "Unique identifier for the team"
          },
          "teamName": {
            "type": "string",
            "description": "Name of the team"
          },
          "logo": {
            "type": "string",
            "description": "URL of the team's logo"
          },
          "description": {
            "type": "string",
            "description": "Description of the team"
          },
          "foundedYear": {
            "type": "number",
            "description": "Year the team was founded"
          },
          "stadium": {
            "type": "string",
            "description": "Name of the team's stadium"
          },
          "jerseys": {
            "type": "object",
            "properties": {
              "home": {
                "type": "string",
                "description": "URL of the home jersey"
              },
              "away": {
                "type": "string",
                "description": "URL of the away jersey"
              },
              "third": {
                "type": "string",
                "description": "URL of the third jersey"
              }
            }
          },
          "paymentVerified": {
            "type": "boolean",
            "description": "Status of payment verification"
          },
          "paymentReceipt": {
            "type": "array",
            "items": {
              "type": "string",
              "description": "ID of the payment receipt",
              "example": "ObjectId"
            }
          },
          "managerId": {
            "type": "string",
            "description": "ID of the manager",
            "example": "ObjectId"
          },
          "players": {
            "type": "array",
            "items": {
              "type": "string",
              "description": "ID of the player",
              "example": "ObjectId"
            }
          },
          "officials": {
            "type": "array",
            "items": {
              "type": "string",
              "description": "ID of the official",
              "example": "ObjectId"
            }
          }
        },
        "required": ["teamName"]
      },

